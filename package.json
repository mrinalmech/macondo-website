{
  "name": "macondo-website",
  "private": true,
  "description": "Website for www.macondogames.com",
  "version": "1.0.0",
  "authors": [
    "Mrinal Mech <mrinalmech@gmail.com> (http://mrinalmech.com)"
  ],
  "license": "0BSD",
  "scripts": {
    "presskit-build": "presskit build ./src/press -o ./public/press -L",
    "presskit-watch": "presskit build ./src/press -o ./public/press -L --watch",
    "build": "gatsby build && npm run presskit-build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:unit": "jest --passWithNoTests",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:unit:watch": "npm run test:unit -- --watch",
    "cy:open": "cypress open --browser chrome --e2e",
    "cy:open:prod": "cross-env CYPRESS_baseUrl=http://localhost:9000 cypress open --browser chrome --e2e",
    "cy:run": "cross-env CYPRESS_baseUrl=http://localhost:9000 cypress run --browser chrome",
    "test:e2e": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:prod": "cross-env CYPRESS_SUPPORT=y npm run build && start-server-and-test serve http://localhost:9000 cy:open:prod",
    "test:e2e:ci": "cross-env CYPRESS_SUPPORT=y npm run build && start-server-and-test serve http://localhost:9000 cy:run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "autoprefixer": "^10.2.5",
    "bootstrap": "^4.6.0",
    "clsx": "^2.0.0",
    "gatsby": "^5.11.0",
    "gatsby-plugin-image": "^3.11.0",
    "gatsby-plugin-manifest": "^5.12.3",
    "gatsby-plugin-offline": "^6.12.3",
    "gatsby-plugin-sass": "^6.11.0",
    "gatsby-plugin-sharp": "^5.11.0",
    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-transformer-sharp": "^5.11.0",
    "jquery": "^3.7.1",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-focus": "^5.0.1",
    "react": "^18.2.0",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^18.2.0",
    "react-use-timeout": "^1.0.0",
    "sass": "^1.63.6",
    "sharp": "^0.32.4",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.2",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.7",
    "@types/jquery": "^3.5.25",
    "axe-core": "^4.8.2",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.12.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.4.0",
    "cypress-axe": "^1.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "presskit": "^0.13.0",
    "start-server-and-test": "^2.0.2",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrinalmech/macondo-website"
  }
}
